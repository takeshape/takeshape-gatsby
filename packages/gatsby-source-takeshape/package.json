{
  "name": "gatsby-source-takeshape",
  "main": "./index.js",
  "types": "./src/index.d.ts",
  "version": "1.1.3",
  "engines": {
    "node": ">=10"
  },
  "author": {
    "name": "TakeShape",
    "email": "contact@takeshape.io"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/takeshape/takeshape-gatsby/tree/trunk/packages/gatsby-source-takeshape"
  },
  "scripts": {
    "build": "npm run clean && tsc --build",
    "build:ci": "npm run build",
    "clean": "rimraf ./dist *.tsbuildinfo",
    "develop": "tsc --watch --preserveWatchOutput",
    "lint": "eslint src -c ./.eslintrc.js --ignore-path .eslintignore",
    "pre-push": "lint-staged",
    "prepublish": "npm run build",
    "reset": "npm run clean && rimraf ./node_modules",
    "test": "jest",
    "test:changed": "npm run test -- --changedSince=trunk",
    "watch": "tsc --watch --preserveWatchOutput"
  },
  "keywords": [
    "gatsby",
    "gatsby-plugin",
    "gatsby-source",
    "takeshape"
  ],
  "license": "MIT",
  "dependencies": {
    "apollo-link": "^1.2.14",
    "apollo-link-http": "^1.5.17",
    "dataloader": "^2.0.0",
    "flush-promises": "^1.0.2",
    "gatsby-core-utils": "^1.3.14",
    "graphql": "14.7.0",
    "graphql-tools": "^6.0.18",
    "imgix-core-js": "^2.3.1",
    "node-fetch": "^2.6.0",
    "p-throttle": "^3.1.0",
    "pusher-js": "^7.0.0",
    "uuid": "^8.3.0"
  },
  "devDependencies": {
    "gatsby": "^2.24.47",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "rimraf": "^3.0.2",
    "typescript": "^4.0.0"
  },
  "peerDependencies": {
    "gatsby": "^2.24.47",
    "react": "^16.13.1",
    "react-dom": "^16.13.1"
  }
}
