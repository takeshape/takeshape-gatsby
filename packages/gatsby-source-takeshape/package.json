{
  "name": "gatsby-source-takeshape",
  "main": "./index.js",
  "types": "./dist/index.d.ts",
  "version": "2.0.0",
  "engines": {
    "node": ">=10"
  },
  "author": {
    "name": "TakeShape",
    "email": "contact@takeshape.io"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/takeshape/takeshape-gatsby/tree/trunk/packages/gatsby-source-takeshape"
  },
  "scripts": {
    "build": "tsc --build",
    "clean": "rimraf ./dist *.tsbuildinfo",
    "develop": "tsc --watch --preserveWatchOutput",
    "lint": "eslint src -c ./.eslintrc.js",
    "pre-push": "lint-staged",
    "prepare": "npm run clean && npm run build",
    "reset": "npm run clean && rimraf ./node_modules",
    "test": "jest",
    "test:changed": "npm run test -- --changedSince=trunk",
    "watch": "tsc --watch --preserveWatchOutput"
  },
  "keywords": [
    "gatsby",
    "gatsby-plugin",
    "gatsby-source",
    "takeshape"
  ],
  "license": "MIT",
  "dependencies": {
    "@apollo/client": "^3.3.14",
    "@graphql-tools/delegate": "^7.1.2",
    "@graphql-tools/links": "^7.0.3",
    "@graphql-tools/merge": "^6.2.11",
    "@graphql-tools/utils": "^7.0.2",
    "@graphql-tools/wrap": "^7.0.1",
    "dataloader": "^2.0.0",
    "flush-promises": "^1.0.2",
    "gatsby-core-utils": "^1.3.14",
    "imgix-core-js": "^2.3.1",
    "node-fetch": "^2.6.0",
    "p-throttle": "^3.1.0",
    "pusher-js": "^7.0.0",
    "uuid": "^8.3.0"
  },
  "devDependencies": {
    "gatsby": "^3.2.1",
    "graphql": "^15.4.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "rimraf": "^3.0.2",
    "typescript": "^4.0.0"
  },
  "peerDependencies": {
    "gatsby": "^2.24.47",
    "react": "^16.13.1",
    "react-dom": "^16.13.1"
  }
}
