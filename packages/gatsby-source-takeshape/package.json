{
  "name": "gatsby-source-takeshape",
  "main": "./index.js",
  "types": "./src/index.d.ts",
  "version": "0.0.2",
  "engines": {
    "node": ">=8.0.0"
  },
  "scripts": {
    "build": "npm run clean && tsc --build",
    "check": "tsc --skipLibCheck --noEmit",
    "clean": "rimraf ./dist *.tsbuildinfo",
    "lint": "eslint -c .eslintrc ./src/**/*",
    "reset": "npm run clean && rimraf ./node_modules",
    "watch": "tsc --watch --preserveWatchOutput",
    "test": "cross-env BABEL_ENV=test jest src",
    "test-changed": "npm run test -- --changedSince=master"
  },
  "keywords": [
    "gatsby",
    "gatsby-plugin",
    "gatsby-source",
    "takeshape"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/takeshape/gatsby-source-takeshape"
  },
  "license": "MIT",
  "dependencies": {
    "@graphql-tools/links": "^6.0.15",
    "@graphql-tools/wrap": "^6.0.15",
    "apollo-link": "^1.2.14",
    "apollo-link-http": "^1.5.17",
    "dataloader": "^2.0.0",
    "gatsby-core-utils": "^1.3.14",
    "node-fetch": "^2.6.0",
    "uuid": "^8.3.0"
  },
  "devDependencies": {
    "@graphql-tools/delegate": "^6.0.15",
    "@graphql-tools/utils": "^6.0.15",
    "@types/jest": "^26.0.8",
    "@types/node": "^14.0.27",
    "@types/node-fetch": "^2.5.7",
    "@types/uuid": "^8.0.0",
    "@typescript-eslint/eslint-plugin": "^3.6.1",
    "@typescript-eslint/parser": "^3.7.1",
    "cross-env": "^5.2.1",
    "gatsby": "^2.24.14",
    "graphql": "14.7.0",
    "jest": "^26.1.0",
    "prettier": "^2.0.5",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "rimraf": "^3.0.2",
    "ts-jest": "^26.1.2",
    "typescript": "^4.0.0-beta"
  },
  "peerDependencies": {
    "gatsby": "^2.0.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1"
  },
  "jest": {
    "transform": {
      "^.+\\.jsx?$": "babel-jest",
      "^.+\\.tsx?$": "ts-jest"
    }
  }
}
