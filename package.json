{
  "name": "takeshape-gatsby",
  "version": "0.0.2",
  "engines": {
    "node": ">=8.0.0"
  },
  "scripts": {
    "reset": "pnpm run reset --recursive --parallel && rimraf node_modules pnpm-lock.yaml",
    "watch": "pnpm run build --filter gatsby-source-sanity && pnpm run watch --recursive --parallel"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/takeshape/gatsby"
  },
  "devDependencies": {
    "@types/jest": "^26.0.4",
    "@types/node": "^14.0.27",
    "@types/node-fetch": "^2.5.7",
    "@types/uuid": "^8.0.0",
    "@typescript-eslint/eslint-plugin": "^3.6.1",
    "@typescript-eslint/parser": "^3.7.1",
    "eslint": "^7.4.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-jest": "^23.18.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.20.5",
    "jest": "^26.1.0",
    "prettier": "^2.0.5",
    "rimraf": "^3.0.2",
    "ts-jest": "^26.1.2",
    "typescript": "^4.0.0-beta"
  },
  "jest": {
    "transform": {
      "^.+\\.jsx?$": "babel-jest",
      "^.+\\.tsx?$": "ts-jest"
    }
  }
}
