{
  "name": "takeshape-gatsby",
  "version": "0.0.0",
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "commit": "git-cz --no-verify",
    "develop": "pnpm run build --filter gatsby-source-takeshape && pnpm run develop --recursive --parallel",
    "preinstall": "npx only-allow pnpm",
    "reset": "pnpm run reset --recursive --parallel && rimraf node_modules pnpm-lock.yaml"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/takeshape/takeshape-gatsby"
  },
  "husky": {
    "hooks": {
      "pre-push": "pnpm run pre-push --parallel --filter ...[origin/master]",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "config": {
    "commitizen": {
      "path": "git-cz"
    }
  },
  "devDependencies": {
    "@commitlint/cli": "^9.1.1",
    "@commitlint/config-conventional": "^9.1.1",
    "@types/jest": "^26.0.8",
    "@types/node": "^14.0.27",
    "@types/node-fetch": "^2.5.7",
    "@types/uuid": "^8.0.0",
    "@typescript-eslint/eslint-plugin": "^3.7.1",
    "@typescript-eslint/parser": "^3.7.1",
    "eslint": "^7.6.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-jest": "^23.20.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.20.5",
    "husky": "^4.2.5",
    "jest": "^26.2.2",
    "jest-fetch-mock": "^3.0.3",
    "lint-staged": "^10.2.11",
    "prettier": "^2.0.5",
    "rimraf": "^3.0.2",
    "ts-jest": "^26.1.4",
    "typescript": "^4.0.0-beta"
  }
}
